{
  "name": "react-ts-express-starter",
  "version": "1.0.0",
  "description": "React + TS + Express starter kit with minimal dependencies",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon --exec \"npm run build && ts-node server/index.tsx\"",
    "build": "tsc && webpack ./.build/client-script.js -o ./tmp --mode production",
    "start": "node .build/server.js"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/react": "^17.0.40",
    "@types/react-dom": "^17.0.13",
    "nodemon": "^2.0.15",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.2",
    "webpack": "^5.70.0",
    "webpack-cli": "^4.9.2"
  },
  "dependencies": {
    "express": "^4.17.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  }
}
